<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="user" table="userstable">
        <meta attribute="class-description">
            This class contains the employee detail.
        </meta>
        <id name="userid" type="string" column="userID">
            <generator class="native"/>
        </id>
        <property name="scheme" column="scheme" type="string"/>
        <property name="timeTaken1" column="timeTaken1" type="float"/>
        <property name="state1" column="state1" type="boolean"/>
        <property name="timeTaken2" column="timeTaken2" type="float"/>
        <property name="state2" column="state2" type="boolean"/>
        <property name="timeTaken3" column="timeTaken3" type="float"/>
        <property name="state3" column="state3" type="boolean"/>
        <property name="timeTaken4" column="timeTaken4" type="float"/>
        <property name="state4" column="state4" type="boolean"/>
        <property name="timeTaken5" column="timeTaken5" type="float"/>
        <property name="state5" column="state5" type="boolean"/>
        <property name="timeTaken6" column="timeTaken6" type="float"/>
        <property name="state6" column="state6" type="boolean"/>
        <property name="timeTaken7" column="timeTaken7" type="float"/>
        <property name="state7" column="state7" type="boolean"/>
        <property name="timeTaken8" column="timeTaken8" type="float"/>
        <property name="state8" column="state8" type="boolean"/>
    </class>
</hibernate-mapping>